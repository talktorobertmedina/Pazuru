<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import app.layout.Tab?>

<Tab fx:id="tab" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<Accordion>
		<panes>
			<TitledPane text="Containers" animated="false">
				<content>
					<ListView fx:id="containersListView"/>
				</content>
			</TitledPane>
			<TitledPane text="Controls" animated="false">
				<content>
					<ListView fx:id="controlsListView"/>
				</content>
			</TitledPane>
			<TitledPane text="Shapes" animated="false">
				<content>
					<ListView fx:id="shapesListView"/>
				</content>
			</TitledPane>
		</panes>
	</Accordion>
</Tab>
